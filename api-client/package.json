{
  "name": "api-client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format:fix": "prettier --write \"src/**/*.{ts,tsx}\"",
    "prepare": "cd .. && husky api-client/.husky",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "lint-staged": {
    "*.tsx": [
      "npm run format:fix",
      "npm run lint"
    ],
    "*.ts": [
      "npm run format:fix",
      "npm run lint"
    ]
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.0",
    "@monaco-editor/react": "^4.6.0",
    "@reduxjs/toolkit": "^2.2.7",
    "@types/lodash": "^4.17.7",
    "firebase": "^10.13.0",
    "lodash": "^4.17.21",
    "next": "14.2.5",
    "next-intl": "^3.17.4",
    "react": "^18",
    "react-dom": "^18",
    "react-firebase-hooks": "^5.1.1",
    "react-hook-form": "^7.52.2",
    "react-icons": "^5.3.0",
    "react-redux": "^9.1.2",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^2.0.5",
    "@vitest/ui": "^2.0.5",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.1.4",
    "jsdom": "^24.1.1",
    "lint-staged": "^15.2.9",
    "monaco-editor": "^0.51.0",
    "prettier": "^3.3.3",
    "typescript": "^5",
    "vitest": "^2.0.5"
  }
}
